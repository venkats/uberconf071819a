{
  "name": "lab",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "watch": {
    "runmocha": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js",
      "quiet": true
    }
  },
  "scripts": {
    "start": "lite-server",
    "test": "npm-watch",
    "build": "NODE_ENV=development webpack --watch",
    "runmocha": "NODE_ENV=test mocha --require setup/setup.js --require babel-core/register --require babel-register --reporter mocha-clearscreen-reporter",
    "coverage": "NODE_ENV=test istanbul cover _mocha -- --require setup/setup.js --require babel-core/register --require babel-register",
    "eslint": "eslint src test"
  },
  "author": "",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-3",
      "react-app"
    ],
    "plugins": [
      [
        "transform-es2015-classes",
        {
          "loose": true
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.8.2",
    "istanbul": ">1.0.0-alpha",
    "jsdom": "^10.0.0",
    "lite-server": "^2.3.0",
    "mocha": "^5.2.0",
    "mocha-clearscreen-reporter": "^1.0.1",
    "npm-watch": "^0.3.0",
    "sinon": "^5.0.10",
    "source-map-loader": "^0.2.3",
    "webpack": "^4.10.2",
    "webpack-cli": "^2.1.4"
  },
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0"
  }
}
